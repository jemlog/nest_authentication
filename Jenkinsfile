pipeline {
  
  agent any
  
  stages {
    
    stage("build") {
        
      steps {
        echo 'building hello new change.'
      }
  }
     stage("test") {
        
      steps {
        echo 'testing...'
      }
  }
     stage("deploy") {
        
      steps {
        echo 'deploying...'
      }
  }
  }
}
